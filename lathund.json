{
  "command": "git branch -D <branch>",
  "explanation": "Tvingar borttagning av en branch."
}
{
  "command": "git stash",
  "explanation": "Sparar undan ändringar temporärt."
}
{
  "command": "git stash clear",
  "explanation": "Tar bort alla sparade stashar."
}
{
  "command": "git show <commit-hash>",
  "explanation": "Visar detaljer om en specifik commit."
}
{
  "command": "git merge <branch>",
  "explanation": "Slår ihop en annan branch med den aktuella."
}
{
  "command": "git remote -v",
  "explanation": "Visar länkade fjärr-repositories."
}
{
  "command": "git remote add origin <url>",
  "explanation": "Länkar ett lokalt repository till en fjärrserver."
}
{
  "command": "git push origin <branch>",
  "explanation": "Skickar upp ändringar till en specifik branch på fjärr-repot."
}
